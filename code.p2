import random

secret_number = random.randint(1, 10)
attempts = 3

print("I'm thinking of a number between 1 and 10.")

while attempts > 0:
    guess = int(input("Your guess: "))

    if guess == secret_number:
        print("Correct! ğŸ‰")
        break
    else:
        attempts -= 1
        print("Wrong guess.")

if attempts == 0:
    print("Out of tries! The number was:", secret_number)
